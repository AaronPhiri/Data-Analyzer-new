<h1>Listing available_systems</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @available_systems.each do |available_system| %>
      <tr>
        <td><%= link_to 'Show', available_system %></td>
        <td><%= link_to 'Edit', edit_available_system_path(available_system) %></td>
        <td><%= link_to 'Destroy', available_system, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Available system', new_available_system_path %>
